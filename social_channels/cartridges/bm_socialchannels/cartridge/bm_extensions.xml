            <pipeline name="BM_TikTok-EnableShopperActivities" />
            <pipeline name="BM_TikTok-DisableShopperActivities" />
		</sub-pipelines>
        <icon>icons/tiktok-logo.svg</icon>
	</menuaction>
    <menuaction id="social_channels_snapchat" menupath="social_channels_customadminmenuextension" position="700" site="true">
